type Error = variant {
  MemoryError;
  UniqueViolation;
  DimensionMismatch;
  NotFound;
};
type Result = variant { Ok; Err : Error };
type Result_2 = variant { Ok : vec text; Err : Error };

service : {
  "process_text" : (nat64, text) -> (vec nat32);
  "find_similar" : (text, int32) -> (vec text);
};
